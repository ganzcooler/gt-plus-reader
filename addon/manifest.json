{

  "manifest_version": 3,
  "name": "GT Plus Reader",
  "version": "1.3",

  "description": "Entfernt die Paywall auf Plus-Artikeln des GT.",

  "icons": {
    "32": "gtpr_32.png",
    "48": "gtpr_48.png",
    "64": "gtpr_64.png",
    "128": "gtpr_128.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.goettinger-tageblatt.de/*"],
      "js": ["gt_reader.js"]
    }
  ]

}
